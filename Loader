if not game:IsLoaded() then
    game.Loaded:Wait()
end

-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Load main script
local success, result = pcall(function()
    return loadstring(game:HttpGet('https://raw.githubusercontent.com/PuneetGOTO/AimbotUI/main/AimbotUI.lua'))()
end)

if not success then
    warn("Failed to load Aimbot: " .. tostring(result))
end
